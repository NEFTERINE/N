<!-- onchange="this.form.submit()" -->

function mostrarPorId($id)
{
$consulta = "SELECT * FROM promotor WHERE pk_promotor = {$id} AND estatus_promotor = 1";
$resultado = $this->conexion->query($consulta);
return $resultado;
}
function actualizar($pk_promotor, $fk_persona, $fk_lider_coordinador, $fk_asistencia_e, $fk_rol)
{
$consulta = "UPDATE promotor SET fk_persona = '{$fk_persona}', fk_lider_coordinador = '{$fk_lider_coordinador}', fk_asistencia_e = '{$fk_asistencia_e}', fk_rol = '{$fk_rol}' WHERE pk_promotor = {$pk_promotor}";
$respuesta = $this->conexion->query($consulta);
return $respuesta;
}
function eliminar($id)
{
$consulta = "UPDATE promotor SET estatus_promotor = 0 WHERE pk_promotor = {$id}";
$respuesta = $this->conexion->query($consulta);
return $respuesta;
}

function buscarPorPersona($fk_persona)
{
$consulta = "SELECT * FROM promotor WHERE fk_persona = '{$fk_persona}' AND estatus_promotor = 1";
$resultado = $this->conexion->query($consulta);
return $resultado;
}
function buscarPorLiderCoordinador($fk_lider_coordinador)
{
$consulta = "SELECT * FROM promotor WHERE fk_lider_coordinador = '{$fk_lider_coordinador}' AND estatus_promotor = 1";
$resultado = $this->conexion->query($consulta);
return $resultado;
}

function buscarPorAsistencia($fk_asistencia_e)
{
$consulta = "SELECT * FROM promotor WHERE fk_asistencia_e = '{$fk_asistencia_e}' AND estatus_promotor = 1";
$resultado = $this->conexion->query($consulta);
return $resultado;
}
function buscarPorRol($fk_rol)
{
$consulta = "SELECT * FROM promotor WHERE fk_rol = '{$fk_rol}' AND estatus_promotor = 1";
$resultado = $this->conexion->query($consulta);
return $resultado;
}
function buscarPorId($id)
{
$consulta = "SELECT * FROM promotor WHERE pk_promotor = {$id} AND estatus_promotor = 1";
$resultado = $this->conexion->query($consulta);
return $resultado;
}











